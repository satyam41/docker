{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/admin/page.jsx",
    "app/layout.jsx",
    "app/page.jsx",
    "lib/utils.js",
    "lib/utils.js",
    "hooks/use-toast.js",
    "hooks/use-mobile.js",
    "components/ui/use-toast.js",
    "components/ui/use-mobile.jsx",
    "components/ui/tooltip.jsx",
    "components/ui/toggle.jsx",
    "components/ui/toggle-group.jsx",
    "components/ui/toaster.jsx",
    "components/ui/toast.jsx",
    "components/ui/textarea.jsx",
    "components/ui/tabs.jsx",
    "components/ui/table.jsx",
    "components/ui/switch.jsx",
    "components/ui/spinner.jsx",
    "components/ui/sonner.jsx",
    "components/ui/slider.jsx",
    "components/ui/skeleton.jsx",
    "components/ui/sidebar.jsx",
    "components/ui/sidebar.jsx",
    "components/ui/sheet.jsx",
    "components/ui/separator.jsx",
    "components/ui/select.jsx",
    "components/ui/scroll-area.jsx",
    "components/ui/resizable.jsx",
    "components/ui/radio-group.jsx",
    "components/ui/progress.jsx",
    "components/ui/popover.jsx",
    "components/ui/pagination.jsx",
    "components/ui/navigation-menu.jsx",
    "components/ui/menubar.jsx",
    "components/ui/label.jsx",
    "components/ui/kbd.jsx",
    "components/ui/item.jsx",
    "components/ui/input.jsx",
    "components/ui/input-otp.jsx",
    "components/ui/input-group.sx",
    "components/ui/hover-card.jsx",
    "components/ui/form.jsx",
    "components/ui/field.jsx",
    "components/ui/empty.jsx",
    "components/ui/dropdown-menu.jsx",
    "components/ui/drawer.jsx",
    "components/ui/dialog.jsx",
